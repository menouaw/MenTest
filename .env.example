# ==========================================================
# 1. Application & Server
# ==========================================================
PORT=
MENTEST_API_URL=http://localhost:8000/api

# ==========================================================
# 2. Global LLM Provider Defaults (fallback values)
#    – These can be overridden per-agent in section 6
# ==========================================================
OPENAI_ENDPOINT=https://api.openai.com/v1
OPENAI_API_KEY="your_openai_api_key_here"

ANTHROPIC_ENDPOINT=https://api.anthropic.com
ANTHROPIC_API_KEY=

AZURE_OPENAI_ENDPOINT=https://your-endpoint.openai.azure.com/
AZURE_OPENAI_API_KEY=

GOOGLE_API_KEY=

DEEPSEEK_ENDPOINT=https://api.deepseek.com
DEEPSEEK_API_KEY=

OLLAMA_ENDPOINT=http://localhost:11434

GROQ_API_KEY=

# ==========================================================
# 3. Cloud & Misc Credentials
# ==========================================================
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1

# ==========================================================
# 4. Redis Configuration (used for Site Explorer state)
# ==========================================================
REDIS_HOST="localhost"
REDIS_PORT="6379"

# ==========================================================
# 5. Browser-Use Settings
# ==========================================================
# Logging level: result | debug | info
BROWSER_USE_LOGGING_LEVEL=info

# Chrome remote-debugging (override if needed)
CHROME_PATH=
CHROME_USER_DATA=
CHROME_DEBUGGING_HOST=localhost
CHROME_DEBUGGING_PORT=9222

# ==========================================================
# 6. Agent-Specific LLM Configuration
#    – Override provider/model/key per agent if different
# ==========================================================
# QA Agent
qa_provider=openai
qa_model_name=gpt-4o
qa_base_url=https://api.openai.com/v1
qa_api_key=

# Planner Agent
plan_provider=openai
plan_model_name=gpt-4o
plan_base_url=https://api.openai.com/v1
plan_api_key=

# Explorer Agent
explore_provider=openai
explore_model_name=gpt-4o
explore_base_url=https://api.openai.com/v1
explore_api_key=

# Report Agent
report_provider=openai
report_model_name=gpt-4o
report_base_url=https://api.openai.com/v1
report_api_key=

